pipeline {
  agent any

  stages {
    stage('Pull Changes') {
        echo 'Antes de ejecutar'
        sh 'pwd'
        sh 'ls -la'
        git branch: 'main',
           credentialsId: 'token-git',
           url: 'https://github.com/Antoniojesus1999/nestjs-api-taller.git'
    }

      echo('Despues de ejcutar')
      sh 'ls -la'
      sh 'pwd'

  }
}
