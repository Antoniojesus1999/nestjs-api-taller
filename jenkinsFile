pipeline {
  agent any
  stages {
    stage('Pull Changes') {

      steps {
                  echo 'clone the repo'
                  sh 'ls -la'
                  sh 'ls -la /var/lib/jenkins/workspace'
                  sh 'pwd'
                  sh 'rm -fr nestjs-api-taller'
                  sh 'ls -la /var/lib/jenkins/workspace'
                  sh 'git clone https://github.com/Antoniojesus1999/nestjs-api-taller.git'
                  sh 'ls -la /var/lib/jenkins/workspace'
                  sh 'ls -la /var/lib/jenkins/workspace/nestjs-api-taller'

      }
    }

  }
}
